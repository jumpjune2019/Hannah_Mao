package phone;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		String[][]phoneBook= new String[5][3]; //[row][column]
		phoneBook[0][0]="Name";
		phoneBook[0][1]="Phone";
		phoneBook[0][2]="City";
		
		Scanner s= new Scanner(System.in);
		
		for (int i = 0; i < phoneBook.length-1; i++) {
			for (int j = 0; j < phoneBook[i].length; j++) {
				phoneBook[i+1][j]=s.nextLine();
				
		    }
			//System.out.println();
	     }	
		
		for (int i = 0; i < phoneBook.length-1; i++) {
			for (int j = 0; j < phoneBook[i].length; j++) {
				phoneBook[i+1][j]=s.nextLine();
				System.out.printf("%-24s", phoneBook[i][j]);
				
        
		    }
			System.out.println();
	     }	

	}

}
