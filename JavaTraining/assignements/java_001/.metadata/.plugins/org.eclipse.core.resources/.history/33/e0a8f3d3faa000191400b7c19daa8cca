import java.util.Arrays;

public class Match {
	
	String SearchArray[];
	String key;
	boolean foundcase;
	boolean result=false;

	
	boolean arrayHasExactMatch(String SearchArray[],String key, boolean foundcase ) {
	
		//case sensitive
		if(foundcase==true) {
			
			if(Arrays.asList(SearchArray).contains(key)) {
		
				result=true;
		
			}
		}
		
		
		//case insensitive
		else if(foundcase==false) {
			
			for(int i=0; i< SearchArray.length; i++ ) {
				
				//SearchArray[i].toUpperCase();
			    //key=key.toUpperCase();
			    
			    if((SearchArray[i].toUpperCase()).equals(key.toUpperCase())) {
			    	result=true;
			    	
			    }
			    
			    // result=false;
			
			}		
			
		}
		
		return result;
		
	}

}
