package java1;

public class Coinflip {
	
    double Num;
    String result;
  
    
	 
	 
	double getNum() {
		
		Num=Math.floor(Math.random()*(2-1+1)+1);
		 
        return Num;
        
		
	}
	
	
    void setNum(double NewNum) {
	   this.Num= NewNum;
		
       	
	}
	
	void setNum(String result) {
		
		if (Num==1.0) {
			
			result="Head";
			
			 System.out.println("The Coin flip is Head");
			
		}
		
		else if(Num==2.0) {
			
			 
			 result="Tail";
			 
			 System.out.println("The Coin fip is Tail");
			 
			
			
		}
		
		
	}
		
		
	//}

	
	
	
	
	
	
	

}
