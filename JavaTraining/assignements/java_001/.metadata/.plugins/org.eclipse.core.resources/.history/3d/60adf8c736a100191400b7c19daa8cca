
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int[][] matrix = new int[6][6];
		
		//initialize the matrix
		
		
		for (int i = 0; i < 6; i++) { 
            for (int j = 0; j < 6; j++) { 
            	
               matrix[i][j] =0; 
            } 
  
            
        } 
		
		
		//print the matrix
		for (int i = 0; i < 6; i++) { 
            for (int j = 0; j < 6; j++) { 
            	
                System.out.print(matrix[i][j] + " "); 
            } 
  
            System.out.println(); 
        } 
		
		

	}

}
