import java.util.Arrays;

public class Match {

	public Match() {
		// TODO Auto-generated constructor stub
	}
	
	
	
	boolean foundcase;
	boolean result;
	String SearchArray[];
	String key;
	

//	public arrayHasExactMatch() {
//		// TODO Auto-generated constructor stub
//		
//		
//	}
	
	boolean arrayHasExactMatch(String SearchArray[],String key, boolean foundcase ) {
		result=false;
		
		if(Arrays.asList(SearchArray).contains(key)) {
		
		result=true;
		}
		
		return result;
		
	}

}
