package basicsorting;

import java.util.Arrays;
import java.util.Collections;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Integer[] numArr = {10,2,4,15,7,9,1,21};
		String[] strArr = {"claude", "Phil", "lois","clark","Arthur", "Mera","bruce"};

		
		MyTransform<Integer> ascArray = (v)->{ 
			
			Arrays.sort(v);
			
		};
		
		ascArray.transform(numArr);
		
		System.out.printf("Modified numArr: %s", Arrays.toString(numArr));
		
        
		MyTransform<Integer> descArray = (v)->{
        	
    			
        		Arrays.sort(v, Collections.reverseOrder()); 
    			
    			
    		};
    		
    	descArray.transform(numArr);
    		
    	System.out.printf("Modified numArr: %s", Arrays.toString(numArr));
    		
			
		};
		
		
		
	}

}
